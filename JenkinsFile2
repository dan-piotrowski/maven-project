pipeline {
    agent any 
    stages {
        stage('Stage 1') {
            steps {
                echo 'Hello world! From VCS...' 
            }
        }
        stage('Stage 2') {
            steps {
                echo 'This is stage 2.' 
            }
        }
        stage('Stage 3') {
            steps {
                echo 'And stage 3 madafakka!' 
                echo '...and eanother echo for stage 3.' 
            }
        }
    }
}
